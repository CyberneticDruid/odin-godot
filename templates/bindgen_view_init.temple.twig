package init

{% if this.parent_package == "core" %}
import "godot:core"
{% end %}
{% for package_ in this.packages %}
import "godot:{{ this.parent_package }}/{{ package_ }}"
{% end %}

init :: proc "contextless" () {
{% if this.parent_package == "core" %}
    core.init_util_procs()
{% end %}
{% for package_ in this.packages %}
    {{ package_ }}.init()
{% end %}
}
